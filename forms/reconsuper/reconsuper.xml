<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>QR activation form</h:title><model odk:xforms-version="1.0.0"><instance><data id="reconsuper" version="22092003"><group_qr><start_time/><end_time/><device_id/><devicephonenum/><todays_date/><username/><chw_id/></group_qr><group_details><worker_type/><first_name/><last_name/><phone_number/></group_details><group_location><note_location/><community_health_unit/><ward/><sub_county/></group_location><group_location_more><note_sub_county/><note_ward/><note_community_health_unit/><village/></group_location_more><group_final><supervisor_id/></group_final><meta><instanceID/></meta></data></instance><instance id="locations" src="jr://file-csv/locations.csv"/><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/group_qr/start_time" type="dateTime"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/group_qr/end_time" type="dateTime"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/group_qr/device_id" type="string"/><bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/data/group_qr/devicephonenum" type="string"/><bind jr:preload="date" jr:preloadParams="today" nodeset="/data/group_qr/todays_date" type="date"/><bind jr:preload="property" jr:preloadParams="username" nodeset="/data/group_qr/username" type="string"/><bind nodeset="/data/group_qr/chw_id" type="barcode"/><bind nodeset="/data/group_details/worker_type" type="string"/><bind nodeset="/data/group_details/first_name" type="string"/><bind nodeset="/data/group_details/last_name" type="string"/><bind nodeset="/data/group_details/phone_number" type="string"/><bind nodeset="/data/group_location/note_location" readonly="true()" type="string"/><bind nodeset="/data/group_location/community_health_unit" type="string"/><bind calculate="pulldata('locations', 'ward', 'community_health_unit',  /data/group_location/community_health_unit )" nodeset="/data/group_location/ward" type="string"/><bind calculate="pulldata('locations', 'sub_county', 'community_health_unit',  /data/group_location/community_health_unit )" nodeset="/data/group_location/sub_county" type="string"/><bind nodeset="/data/group_location_more/note_sub_county" readonly="true()" type="string"/><bind nodeset="/data/group_location_more/note_ward" readonly="true()" type="string"/><bind nodeset="/data/group_location_more/note_community_health_unit" readonly="true()" type="string"/><bind nodeset="/data/group_location_more/village" type="string"/><bind nodeset="/data/group_final" relevant=" /data/group_details/worker_type !='Supervisor'"/><bind nodeset="/data/group_final/supervisor_id" type="barcode"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/></model></h:head><h:body><group appearance="field-list" ref="/data/group_qr"><label>QR details</label><input ref="/data/group_qr/chw_id"><label>Scan the card which you are going to "activate"</label></input></group><group appearance="field-list" ref="/data/group_details"><select1 ref="/data/group_details/worker_type"><label>What kind of worker is this ID card for?</label><item><label>CHW</label><value>CHW</value></item><item><label>CHA</label><value>CHA</value></item><item><label>Supervisor</label><value>Supervisor</value></item></select1><input ref="/data/group_details/first_name"><label>First name of worker whose ID card is being activated</label></input><input ref="/data/group_details/last_name"><label>Last name of worker whose ID card is being activated</label></input><input ref="/data/group_details/phone_number"><label>Phone number of worker whose ID card is being activated</label></input></group><group appearance="field-list" ref="/data/group_location"><input ref="/data/group_location/note_location"><label>Location of worker whose ID card is being activated (placeholder until selection hierarchy is created)</label></input><select1 appearance="autocomplete search('community_health_units')" ref="/data/group_location/community_health_unit"><label>Community Health Unit</label><item><label>community_health_unit</label><value>community_health_unit</value></item></select1></group><group appearance="field-list" ref="/data/group_location_more"><input ref="/data/group_location_more/note_sub_county"><label>Sub-county: <output value=" /data/group_location/sub_county "/></label></input><input ref="/data/group_location_more/note_ward"><label>Sub-county: <output value=" /data/group_location/ward "/></label></input><input ref="/data/group_location_more/note_community_health_unit"><label>Community unit: <output value=" /data/group_location/community_health_unit "/></label></input><input ref="/data/group_location_more/village"><label>CHW village name (If more than one, enter separating by ",")</label></input></group><group appearance="field-list" ref="/data/group_final"><input ref="/data/group_final/supervisor_id"><label>Scan the SUPERVISOR ID card</label></input></group></h:body></h:html>