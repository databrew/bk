<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>Recon C. Household form</h:title><model odk:xforms-version="1.0.0"><instance><data id="reconchousehold" version="22092004"><group_qr><start_location/><start_time/><end_time/><device_id/><devicephonenum/><todays_date/><username/><note_worker_details/><have_wid/><wid_manual/><w_first_name/><w_last_name/><wid_qr/><wid/></group_qr><group_location><note_chw_village_location/><community_health_unit/><ward/><sub_county/></group_location><group_location_more><note_sub_county/><note_ward/><note_community_health_unit/><village/></group_location_more><group_geocode><geoloc_note/><location_geocode/></group_geocode><group_hh_info><note_chw_village_location/><num_hh_members/><note_num_members_warning/><num_hh_members_lt_5/><num_hh_members_bt_5-15/><num_hh_members_gt_15/><all_age_members/><warning_all_members_sum/></group_hh_info><meta><instanceID/></meta></data></instance><instance id="locations" src="jr://file-csv/locations.csv"/><bind nodeset="/data/group_qr/start_location" type="geopoint"/><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/group_qr/start_time" type="dateTime"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/group_qr/end_time" type="dateTime"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/group_qr/device_id" type="string"/><bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/data/group_qr/devicephonenum" type="string"/><bind jr:preload="date" jr:preloadParams="today" nodeset="/data/group_qr/todays_date" type="date"/><bind jr:preload="property" jr:preloadParams="username" nodeset="/data/group_qr/username" type="string"/><bind nodeset="/data/group_qr/note_worker_details" readonly="true()" type="string"/><bind nodeset="/data/group_qr/have_wid" required="true()" type="string"/><bind constraint="regex(.,('^[2]{1}[0-9]{3}'))" jr:constraintMsg="The worker ID must be 4 characters and follow this format: 2###" nodeset="/data/group_qr/wid_manual" relevant=" /data/group_qr/have_wid ='No'" required="true()" type="string"/><bind constraint="regex(.,'^[A-Za-z ]{1,15}$')" jr:constraintMsg="You must enter alphabetical characters only." nodeset="/data/group_qr/w_first_name" relevant=" /data/group_qr/have_wid ='No'" required="true()" type="string"/><bind constraint="regex(.,'^[A-Za-z ]{1,15}$')" jr:constraintMsg="You must enter alphabetical characters only." nodeset="/data/group_qr/w_last_name" relevant=" /data/group_qr/have_wid ='No'" required="true()" type="string"/><bind constraint="string-length(.)=4" jr:constraintMsg="The worker ID must be 4 characters" nodeset="/data/group_qr/wid_qr" relevant=" /data/group_qr/have_wid ='Yes'" required="true()" type="barcode"/><bind calculate="if( /data/group_qr/have_wid ='Yes',  /data/group_qr/wid_qr ,  /data/group_qr/wid_manual )" nodeset="/data/group_qr/wid" type="string"/><bind nodeset="/data/group_location/note_chw_village_location" readonly="true()" type="string"/><bind nodeset="/data/group_location/community_health_unit" required="true()" type="string"/><bind calculate="pulldata('locations', 'ward', 'community_health_unit',  /data/group_location/community_health_unit )" nodeset="/data/group_location/ward" type="string"/><bind calculate="pulldata('locations', 'sub_county', 'community_health_unit',  /data/group_location/community_health_unit )" nodeset="/data/group_location/sub_county" type="string"/><bind nodeset="/data/group_location_more/note_sub_county" readonly="true()" type="string"/><bind nodeset="/data/group_location_more/note_ward" readonly="true()" type="string"/><bind nodeset="/data/group_location_more/note_community_health_unit" readonly="true()" type="string"/><bind nodeset="/data/group_location_more/village" required="true()" type="string"/><bind nodeset="/data/group_geocode/geoloc_note" readonly="true()" type="string"/><bind nodeset="/data/group_geocode/location_geocode" required="true()" type="geopoint"/><bind nodeset="/data/group_hh_info/note_chw_village_location" readonly="true()" type="string"/><bind nodeset="/data/group_hh_info/num_hh_members" required="true()" type="int"/><bind nodeset="/data/group_hh_info/note_num_members_warning" readonly="true()" relevant=" /data/group_hh_info/num_hh_members &gt;15" type="string"/><bind nodeset="/data/group_hh_info/num_hh_members_lt_5" required="true()" type="int"/><bind nodeset="/data/group_hh_info/num_hh_members_bt_5-15" required="true()" type="int"/><bind nodeset="/data/group_hh_info/num_hh_members_gt_15" required="true()" type="int"/><bind calculate=" /data/group_hh_info/num_hh_members_lt_5 + /data/group_hh_info/num_hh_members_bt_5-15 + /data/group_hh_info/num_hh_members_gt_15 " nodeset="/data/group_hh_info/all_age_members" type="string"/><bind nodeset="/data/group_hh_info/warning_all_members_sum" readonly="true()" relevant=" /data/group_hh_info/all_age_members != /data/group_hh_info/num_hh_members " required="true()" type="string"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/><odk:setgeopoint event="odk-instance-first-load" ref="/data/group_qr/start_location"/></model></h:head><h:body><group appearance="field-list" ref="/data/group_qr"><label>Worker details</label><input ref="/data/group_qr/note_worker_details"><label>&lt;span style="color:#9f2e2e"&gt;Worker details&lt;/span&gt;</label></input><select1 appearance="rating_box-fill-f6f6f6-009688-737373-ffffff" ref="/data/group_qr/have_wid"><label>Do you have a QR code with your field worker ID?</label><item><label>Yes</label><value>Yes</value></item><item><label>No</label><value>No</value></item></select1><input appearance="numbers" ref="/data/group_qr/wid_manual"><label>Manually write your 4 digit field worker ID here</label></input><input ref="/data/group_qr/w_first_name"><label>First name</label></input><input ref="/data/group_qr/w_last_name"><label>Last name</label></input><input ref="/data/group_qr/wid_qr"><label>Scan your field worker ID QR code</label></input></group><group appearance="field-list" ref="/data/group_location"><input ref="/data/group_location/note_chw_village_location"><label>&lt;span style="color:#9f2e2e"&gt;Household location&lt;/span&gt;</label></input><select1 appearance="autocomplete search('community_health_units')" ref="/data/group_location/community_health_unit"><label>Community unit (CU)</label><item><label>community_health_unit</label><value>community_health_unit</value></item></select1></group><group appearance="field-list" ref="/data/group_location_more"><input ref="/data/group_location_more/note_sub_county"><label>Sub-county: <output value=" /data/group_location/sub_county "/></label></input><input ref="/data/group_location_more/note_ward"><label>Ward: <output value=" /data/group_location/ward "/></label></input><input ref="/data/group_location_more/note_community_health_unit"><label>Community unit: <output value=" /data/group_location/community_health_unit "/></label></input><input ref="/data/group_location_more/village"><label>Village where this household is located</label></input></group><group appearance="field-list" ref="/data/group_geocode"><input ref="/data/group_geocode/geoloc_note"><label>Stand just outside the front door of the house and geocode the location</label></input><input ref="/data/group_geocode/location_geocode"><label>Geocoding</label></input></group><group appearance="field-list" ref="/data/group_hh_info"><label>Household information</label><input ref="/data/group_hh_info/note_chw_village_location"><label>&lt;span style="color:#9f2e2e"&gt;Household information&lt;/span&gt;</label></input><input ref="/data/group_hh_info/num_hh_members"><label>Number of household members</label></input><input ref="/data/group_hh_info/note_num_members_warning"><label>&lt;span style="color:#9f2e2e"&gt;Please double check that there are over 15 household members&lt;/span&gt;</label></input><input ref="/data/group_hh_info/num_hh_members_lt_5"><label>Number of household members &lt;5 years old</label></input><input ref="/data/group_hh_info/num_hh_members_bt_5-15"><label>Number of household members &gt;= 5 - &lt;15 years old</label></input><input ref="/data/group_hh_info/num_hh_members_gt_15"><label>Number of household members &gt;= 15 years old</label></input><input ref="/data/group_hh_info/warning_all_members_sum"><label>&lt;span style="color:#ff0000"&gt;The sum of all members by age is not equal to the "Number of household members". Please check.&lt;/span&gt;</label></input></group></h:body></h:html>